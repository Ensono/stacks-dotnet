<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
	<LangVersion>10.0</LangVersion>
    <Description>DynamoDB package for common features used throughout the projects.</Description>
    <Company>Amido</Company>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/amido/stacks-dotnet-packages-dynamodb</PackageProjectUrl>
    <RepositoryUrl>https://github.com/amido/stacks-dotnet-packages-dynamodb</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AWSSDK.DynamoDBv2" Version="3.7.300.1" />
    <PackageReference Include="AWSSDK.Extensions.NETCore.Setup" Version="3.7.7" />
    <PackageReference Include="Amido.Stacks.Data.Documents.Abstractions" Version="0.2.15" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="6.0.0" />
  </ItemGroup>
</Project>
