{
    "$schema": "http://json.schemastore.org/template",
    "author": "stacks@ensono.com",
    "classifications": [
        "Stacks",
        "Azure Function",
        "CosmosDb",
        "Worker"
    ],
    "name": "Ensono Stacks Azure Function CosmosDb Worker",
    "identity": "Ensono.Stacks.AzFunc.CosmosDb.Worker.CSharp",
    "groupIdentity": "Ensono.Stacks.AzFunc.CosmosDb.Worker",
    "shortName": "stacks-az-func-cosmosdb-worker",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sourceName": "xxENSONOxx.xxSTACKSxx",
    "preferNameDirectory": true,
    "symbols": {},
    "sources": [
        {
            "source": "../../",
            "include": [
                "build/**",
				"deploy/**",
				"taskctl.yaml",
				"stackscli.yml",
				".gitignore",
				".gitattributes",
                "src/func-cosmosdb-worker/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Application.CQRS.Abstractions/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Configuration/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Configuration.Tests/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Core/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Core.Tests/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Messaging.Azure.ServiceBus/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Messaging.Azure.ServiceBus.Tests/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Messaging.Commands/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Messaging.Events/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Messaging.Handlers/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Testing/**",
				"src/shared/xxENSONOxx.xxSTACKSxx.Shared.Testing.Tests/**"
            ],
            "exclude": [
                "**/[Bb]in/**",
                "**/[Oo]bj/**",
                "**/.template.config/**",
                "**/*.filelist",
                "**/*.user",
                "**/*.lock.json",
                "**/.git/**",
                "**/.vs/**",
                "**/.vscode/**",
                "_rels/**",
                "package/**",
                "**/*.nuspec",
                "*Content_Types*.xml",
                "**/build/azDevOps/aws/**",
                "**/build/azDevOps/gcp/**",
                "**/build/azDevOps/azure/ensono/**",
                "**/build/azDevOps/azure/ci-cqrs-vars.yml",
                "**/build/azDevOps/azure/ci-simple-api-vars.yml",
                "**/build/azDevOps/azure/ci-asb-listener-vars.yml",
                "**/build/azDevOps/azure/ci-aeh-listener-vars.yml",
                "**/build/azDevOps/azure/ci.yml",
                "**/build/github/**",
                "**/deploy/aws/**",
                "**/deploy/gcp/**"
            ],
            "rename": {
                "_gitignore": ".gitignore",
                "_gitattributes": ".gitattributes",
                "build/azDevOps/azure/ci-functions.yml": "build/azDevOps/azure/ci.yml",
				"build/azDevOps/azure/ci-cosmosdb-worker-vars.yml": "build/azDevOps/azure/ci-vars.yml"
            },
            "modifiers": []
        }
    ],
    "SpecialCustomOperations": {
        "**/*.yml": {
            "Operations": [
                {
                    "type": "conditional",
                    "configuration": {
                        "style": "line",
                        "token": "#"
                    }
                }
            ]
        }
    }
}
