{
  "Serilog": {
      "Using": [
          "Serilog.Sinks.Console",
          "Serilog.Sinks.ApplicationInsights"
      ],
      "MinimumLevel": {
          "Default": "Information",
          "Override": {}
      },
      "WriteTo": [
          { "Name": "Console" },
          {
              "Name": "ApplicationInsights",
              "Args": {
                  "telemetryConverter": "Serilog.Sinks.ApplicationInsights.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
              }
          }
      ],
      "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
      "Destructure": [],
      "Properties": {
          "Application": "Menu API"
      }
  },
  "CosmosDb": {
    "DatabaseAccountUri": "https://amidostacksmenucdaukstestx.documents.azure.com:443/",
    "DatabaseName": "Stacks",
    "SecurityKeySecret": {
      "Identifier": "COSMOSDB_KEY",
      "Source": "Environment"
    }
  },
  "TestConfig": "Test value from test file",
  "Values":{
    "Items": [
      "Item 1 from test config",
      "Item 2 from test congig",
      "Item 3 from test config",
      "Item 4 from test config",
      "Item 5 from test config"
    ]
  }
